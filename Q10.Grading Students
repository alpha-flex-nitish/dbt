#include <iostream>
#include<cmath>
#include<bits/stdc++.h>
using namespace std;

int main() {
int i;
int g[i], n;
bool pri = true;

cin >> n;

for(i=0;i<n;i++){
        cin >> g[i];
}

if(g[i]<38||g[i]%5==0||(g[i]%5)<3&&g[i]>40)
{
    g[i] = g[i] + 0;
}
else if((g[i]>38)&&(g[i]%5)>=3&&g[i]%5!=0)
{
    g[i] = ((g[i]/5)+1)*5;
}


for(i=0;i<n;i++){
if (g[i]>=0&&g[i]<=100){
    pri = true;
}
else{
    pri = false;
}
}

if (n>=1&&n<=60){
    pri = true;
}
else{
    pri = false;
}

if(pri==true){
    for (i=0;i<n;i++)
        {
            cout << g[i] << endl;
        }
}
else{
    cout << "invalid input";
}


return 0;
}
