#include<iostream>
#include<cmath>
#include<bits/stdc++.h>
using namespace std;

int main(){

        int i, j, n, arr[10][10], sumlr=0, sumrl=0, diff = 0;
        bool input = true;
        cin >> n ;

        for (i=0;i<n;i++)
        {
            for (j=0;j<n;j++)
            cin >> arr[i][j];
        }
        
        for (i=0;i<n;i++)
        {
            for (j=0;j<n;j++)
            {
            
            if(i==j){
                sumlr += arr[i][j];
                    }
            else {}
            if(i+j==n-1){
                sumrl += arr[i][j];
                        }
            else{}
            }

        }
        cout << "sumLR = " << sumlr << "& sumRL = " << sumrl;

        for (i=0;i<n;i++)
        {
            for (j=0;j<n;j++)
            {
                if(-100<=arr[i][j]&&arr[i][j]<=100)
                    {
                    input = true;
                    cout << i << j <<" is " << "valid input.";
                    }
                else{
                    input = false;
                    i  = n, j = n;
                    }
            }
        }
        if (input==1)
          {

            if(sumlr>sumrl)
               {
                diff = sumlr-sumrl;
               }
            else{
                diff = sumrl-sumlr;
                }
                cout << diff;
           }

        else{
                cout << "invalid input";
            }
          cout <<"value of Input is :" << input;
return 0;
}
