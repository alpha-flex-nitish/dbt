#include<iostream>
#include <bits/stdc++.h>
#include<cmath>
using namespace std;
int main()
{
    long long int n, i, ar[i], sum=0;

    cin >> n;

    if(1<=n&&n<=10){

        for(i=0;i<n;i++)
        {
           cin >> ar[i];

           if(0<=ar[i]&&ar[i]<=10000000000){
            sum += ar[i];
           }
           else{
               i = n+1;
               cout << "invalid input";

           }
           if(i==n+1){

           }
           else{
            cout << sum;
           }
        }
    }
    else{
        cout << "invalid input";

    }
}
