
#include<iostream>
#include<cmath>
#include<bits/stdc++.h>

using namespace std;
//3 2 1 3
int main(){

    int n, i, j, c[i], t=0;

    cin >> n;



    for (i=0;i<n;i++){
        cin >> c[i];
    }

    for (j=0;j<n;j++){
        if(t!=0){i=n+1;j=n+1;}
        else if(t=0){i+=0;}
        for (i=0;i<n;i++){


        if (c[i]==n-j){
            t++;
            cout << " t +1 for i, j = " << i << j << endl;

        }
        else{
            t = t + 0;
        }
    }
    }
    cout << t;


return 0;
}
