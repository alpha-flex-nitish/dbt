#include <cmath>
#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main()
{

    int i;
    int ar[i], n, sum = 0;
    cin >> n;

    for(i = 0; i<n; i++)
    {
            if(((1<=ar[i])&&(ar[i]<=1000))&&((1<=n)&&(n<=1000)))
            {
                    cin >> ar[i];

            }
            else
            {
                cout << "invalid input";
            }
    }



    if(((1<=ar[i])&&(ar[i]<=1000))&&((1<=n)&&(n<=1000)))

    {
    for(i = 0; i<n; i++)
    {
        sum = sum + ar[i];
    }
        cout << sum;
    }
    else
    {
        cout << "invalid input";

    }


    return 0;

}

