#include<iostream>
#include<cmath>
#include <bits/stdc++.h>
using namespace std;

int main(){
        int i=0, n=3, aP=0, bP=0;
        int  a[3], b[3];
        for(i=0;i<n;i++)
        {
            cin >> a[i];
        }
        for(i=0;i<n;i++)
        {
            cin >> b[i];
        }
        for(i=0;i<n;i++)
            {
                if(a[i]<b[i])
                    {
                    bP++;
                    }
                else if(a[i]>b[i])
                    {
                    aP++;
                    }

            }
        cout << aP << " " << bP;
return 0;


}
